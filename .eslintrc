{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "no-console": 1
  },
  "plugins": ["import", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
